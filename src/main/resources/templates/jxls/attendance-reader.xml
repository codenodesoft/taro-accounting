<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet idx="0">
        <section startRow="0" endRow="0" />
        <loop startRow="1" endRow="1" items="attendances" var="attendance" varType="org.soptorshi.domain.AttendanceExcelParser">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">attendance.employeeId</mapping>
                <mapping row="1" col="4">attendance.attendanceDate</mapping>
                <mapping row="1" col="5">attendance.inOutTime</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0" />
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>
